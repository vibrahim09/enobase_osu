      "lib": ["esnext"],
      "skipLibCheck": true,
      "strict": true,
      "esModuleInterop": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "outDir": "./dist"
    },
    "include": ["src/app/api/**/*.ts"],
    "exclude": ["node_modules"]
